          <mxCell style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;spacing=0;fontSize=14;fontStyle=1;spacingLeft=5;" parent="1" vertex="1">
